{
  "name": "Pronotif",
  "version": "0.1.0",
  "description": "Re√ßois tes notifications Pronote directement sur le bureau",
  "manifest_version": 2,
  "icons": {
    "16": "logos/logo16.png",
    "48": "logos/logo48.png",
    "128": "logos/logo128.png" 
  },
  "browser_action" : {
    "default_popup" : "popup.html",
    "default_icon": "logos/logo16.png"
  },

  "background": {
    "scripts": ["jquery-3.5.1.min.js","background.js"]
  },

  "permissions": [
    "storage",
    "notifications"
  ]
}